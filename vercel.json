{
    "version": 2,

  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.2",
      "maxDuration": 10,
      "memory": 512
      }
  },

    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
          "src": "/api/script.php",
          "dest": "/api/script.php"
        }
       
    ]
}
