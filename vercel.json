{

    "version": 2,
    "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python@canary"
    }
  ],
    "rewrites": [ 
      {
        "source": "/(.*)",
        "destination": "public/$1"
      },
      { 
        "source": "/(.*)", 
        "destination": "api/index.py"
      }
     
    ]
  }