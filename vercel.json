{
    "version": 2,
    
    "build":{
        {
            "src": "public/**/*",
            "use": "@vercel/static" 
        }
    },

    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },

        {
            "src": "/(.*)",
            "dest": "/api/script.php"
        }
    ]

}