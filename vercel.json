{
    "version": 2,
 
    "functions": {
        "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    },
  
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        { 
            "src": "/(.*)", 
            "dest": "/api/script.php"
         }
       
    ]
}
