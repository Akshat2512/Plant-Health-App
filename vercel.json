{
    "version": 2,
    "builds": [
      {
        "src": "script.php",
        "use": "@vercel/php"
      },
      {
        "src": "Python_Backend/plant_disease_detection.py",
        "use": "@vercel/python",
        "config": {
        "installCommand": "pip install -r requirements.txt"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/script.php"
      }
    ]
  }