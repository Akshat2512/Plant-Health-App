{
    "version": 2,
    "builds": [
    {
      "src": "PythonBackend/plant_disease_detection.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
    ],
  
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        { 
            "src": "/api/script.php", 
            "dest": "/api/script.php"
         }
    ]
}
