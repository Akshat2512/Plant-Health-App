{
    "version": 2,
    "builds": [
      {
        "src": "PythonBackend/plant_disease_detection.py",
        "use": "@vercel/python",
        "config": {
          "installcommand" :"pip install -r requirements.txt"
        }
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      },
       {
        "src": "script.php",
        "use": "@vercel/docker",
        "config": {
          "dockerfile": "Dockerfile"
        }
      }
    ],
  
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
       
    ]
}
