{

    "version": 2,
    "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  
    "rewrites": [ 
      {
        "source": "/(.*)",
        "destination": "public/$1"
      },
      { 
        "source": "/(.*)", 
        "destination": "api/index.py"
      }
     
    ]
  }