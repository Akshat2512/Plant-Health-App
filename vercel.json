{
    "version": 2,
    "builds": [
      {
        "src": "PythonBackend/plant_disease_detection.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
            "src": "index.html",
            "dest": "/"
          },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
